    <string name="beginner_notes" formatted="false">Twice a week with at least one day rest between training sessions. Keep repeating the same exercises.</string>
    <string name="advanced">Advanced</string>
    <string name="advanced_notes" formatted="false">3 times per week with at least one day rest between training sessions. Keep repeating the same exercises.</string>
